#include "pch.h"
#include <iostream>
#include <stdio.h>
using namespace std;

int main(int argc, char** argv)
{
	int iOut;
	char* pcInp;
	if (argc < 2)
	{
		cout << "Truksta argumentu" << endl;
		return 0;
	}
	pcInp = argv[1]; 
	__asm {
		push eax 
		push ebx
		push ecx
		mov ebx, 0 
		mov ecx, pcInp
		dec ecx 
		ciklas :
		inc ecx 
			mov al, byte ptr[ecx]	
			cmp al, 0 
			jz pabaiga
			sub al, '0' 
			imul ebx, 10 
			add ebx, eax 
			jmp ciklas 
			pabaiga :
		    mov[iOut], ebx  
			pop ecx
			pop ebx
			pop eax
	}
	cout << "Atsakymas: " << iOut << endl;
	return 0;
}
